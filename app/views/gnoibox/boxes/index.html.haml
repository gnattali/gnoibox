#pad-wrapper
  .table-wrapper.section
    .row.head
      .col-md-12
        %h4 コンテンツ一覧

    .row
      %table.table.table-hover
        %thead
          %tr
            %th.col-md-3
              name
            %th.col-md-6
              action
        %tbody
          -gnb.boxes.each do |box|
            %tr
              %td=box.label
              %td
                =link_to '編集', gnoibox_box_items_path(box)
